{
  "name": "nogesma.gihub.io",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "dayjs": "^1.8.14",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.0",
    "prettier-plugin-svelte": "^0.5.0",
    "ramda": "^0.26.1",
    "rollup": "^1.11.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "5.0.0",
    "sanctuary-maybe": "^1.2.0",
    "sirv-cli": "^0.4.0",
    "svelte": "^3.4.2"
  },
  "pre-commit": [
    "code-style"
  ],
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv docs",
    "start:dev": "sirv docs --dev",
    "code-style": "prettier --check **/**.html **/**.js **/**.json **/**.css **/**.svelte --plugin-search-dir=. ./**/*.html"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "endOfLine": "lf",
    "trailingComma": "es5",
    "useTabs": false
  }
}
